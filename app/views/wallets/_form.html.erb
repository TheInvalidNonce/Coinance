<%= form_for wallet, url: {action: action} do |f| %>
  <%= f.hidden_field :user_id, value: current_user.id %>
  <%= f.label "Wallet Name" %>
  <%= f.text_field :name %>
  <%= f.label "Coin Amount" %>
  <%= f.text_field :coin_amount %>
  <%= f.label "User Deposit" %>
  <%= f.text_field :user_deposit %>
  
  
  <%= f.submit "Create Wallet" %>
<% end %>